{
    "name": "deja-angular2",
    "version": "0.0.1-beta4",
    "description": "Angular2 Components Library",
    "homepage": "https://github.com/KillingJokeCHOrg/dejajs",
    "bugs": "https://github.com/KillingJokeCHOrg/dejajs/issues",
    "author": "Team killingJoke.CH",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/KillingJokeCHOrg/dejajs.git"
    },
    "scripts": {
        "drug": "json-server --port 5202 --ro -q ./src/demo-app/services/drug-event-0003-of-0003-2.json",
        "countries": "json-server --port 5201 --ro -q ./src/demo-app/services/countries.json",
        "doc-component": "typedoc --options typedoc.json ./src/component/",
        "doc-common": "typedoc --options typedoc.json ./src/common/",
        "doc": "concurrently \"npm run doc-component\" \"npm run doc-common\"",
        "build-css": "node-sass src/scss/index.scss -o ./src/scss",
        "build": "webpack-dev-server --inline --progress --port 5200 --config webpack.config.dev.js",
        "start": " npm run build-css && concurrently \"npm run countries\" \"npm run drug\" \"npm run build\"",
        "prepublish": "tsc && npm run build-css",
        "build:prod": "del-cli dist &&  npm run build-css && webpack -p --config webpack.config.prod.js",
        "serve": "lite-server && concurrently \"npm run countries\" \"npm run drug\""
    },
    "keywords": [
        "angular2",
        "material",
        "component",
        "grid",
        "datagrid",
        "data-grid",
        "grid",
        "datepicker",
        "colorpicker",
        "messagebox",
        "monaco",
        "select",
        "textarea",
        "tiles",
        "snackbar",
        "demo",
        "demo-app",
        "webpack"

    ],
    "dependencies": {
        "@angular/common": "~2.1.0",
        "@angular/compiler": "~2.1.0",
        "@angular/core": "~2.1.0",
        "@angular/forms": "~2.1.0",
        "@angular/http": "~2.1.0",
        "@angular/material": "^2.0.0-beta.1",
        "@angular/platform-browser": "~2.1.0",
        "@angular/platform-browser-dynamic": "~2.1.0",
        "@angular/router": "~3.1.0",
        "@angular/upgrade": "^2.0.0",
        "bluebird": "^3.4.6",
        "core-js": "^2.4.1",
        "hammerjs": "^2.0.8",
        "json-server": "^0.9.4",
        "material-design-lite": "^1.1.3",
        "moment": "^2.11.2",
        "monaco-editor": "^0.7.0",
        "reflect-metadata": "0.1.8",
        "rxjs": "^5.0.0-rc.3",
        "x2js": "^3.0.1",
        "zone.js": "^0.6.23"
    },
    "devDependencies": {
        "@types/chai": "^3.4.34",
        "@types/core-js": "^0.9.34",
        "@types/hammerjs": "^2.0.33",
        "@types/jasmine": "^2.5.38",
        "@types/node": "^6.0.47",
        "angular2-router-loader": "^0.3.2",
        "angular2-template-loader": "^0.5.0",
        "awesome-typescript-loader": "^2.2.4",
        "concurrently": "^3.1.0",
    "copy-webpack-plugin": "^4.0.1",
        "css-loader": "^0.26.1",
        "del-cli": "^0.2.1",
        "exports-loader": "^0.6.3",
        "file-loader": "^0.9.0",
        "html-loader": "^0.4.4",
        "html-webpack-plugin": "^2.22.0",
        "lite-server": "^2.2.2",
        "node-sass": "^3.12.5",
        "raw-loader": "^0.5.1",
        "resolve-url-loader": "^1.6.0",
        "sass-loader": "^4.0.2",
        "style-loader": "^0.13.1",
        "tslint": "^4.0.2",
        "typedoc": "^0.5.1",
        "typedoc-webpack-plugin": "^1.1.3",
        "typescript": "2.0.10",
        "url-loader": "^0.5.7",
        "webpack": "2.1.0-beta.21",
        "webpack-dev-server": "2.1.0-beta.08",
        "webpack-merge": "^0.14.1"
    }
}